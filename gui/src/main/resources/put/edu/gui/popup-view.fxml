<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16"
      fx:controller="put.edu.gui.controllers.PopupController">
    <Text fx:id="descriptionText" strokeType="OUTSIDE" strokeWidth="0.0" text="description"/>
    <Button mnemonicParsing="false" onAction="#exit" prefWidth="100.0" text="ok">
        <VBox.margin>
            <Insets top="20.0"/>
        </VBox.margin>
    </Button>
</VBox>
